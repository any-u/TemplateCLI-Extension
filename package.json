{
	"name": "tcli",
	"displayName": "TCli",
	"description": "template cli",
	"version": "0.0.7",
	"icon": "images/tcli.png",
	"engines": {
		"vscode": "^1.35.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.create",
		"onCommand:extension.version"
	],
	"main": "./src/extension.js",
	"publisher": "sillyY",
	"repository": "https://github.com/sillyY/TemplateCLI-Extension.git",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Tcli configuration",
			"properties": {
				"tcli.template": {
					"type": "object",
					"default": {
						"react": {
							"root": "/Users/sillyy/学习/tcli模板/index.js",
							"type": ".js",
							"mark": "Index"
						},
						"vue": {
							"root": "/Users/sillyy/学习/tcli模板/index.vue",
							"type": ".vue",
							"mark": "Index"
						}
					},
					"description": "Set template"
				}
			}
		},
		"commands": [
			{
				"command": "extension.create",
				"title": "create",
				"category": "Tcli"
			},
			{
				"command": "extension.version",
				"title": "version",
				"category": "Tcli"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"dependencies": {
		"mkdirp": "^0.5.1"
    },
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"eslint": "^5.13.0",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	}
}
