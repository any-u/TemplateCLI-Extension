{
	"name": "tcli",
	"displayName": "TCli",
	"description": "template cli",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.35.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.create"
	],
	"main": "./src/extension.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Tcli configuration",
			"properties": {
				"tcli.template": {
					"type": "object",
					"default": {
						"react": {
							"root": "",
							"type": ".js"
						},
						"vue": {
							"root": "",
							"type": ".vue"
						}
					},
					"description": "Set template"
				}
			}
		},
		"commands": [
			{
				"command": "extension.create",
				"title": "create",
				"category": "Tcli"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"eslint": "^5.13.0",
		"mkdirp": "^0.5.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	}
}
